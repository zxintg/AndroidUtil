<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/md_switch_thumb_disable" />
    <item android:state_checked="true" android:state_pressed="true" android:drawable="@drawable/md_switch_thumb_on_pressed" />
    <item android:state_focused="true" android:state_checked="true" android:drawable="@drawable/md_switch_thumb_on_pressed" />
    <item android:state_checked="true" android:drawable="@drawable/md_switch_thumb_on_normal" />
    <item android:state_checked="false" android:state_pressed="true" android:drawable="@drawable/md_switch_thumb_off_pressed" />
    <item android:state_focused="true" android:state_checked="false" android:drawable="@drawable/md_switch_thumb_off_pressed" />
    <item android:state_checked="false" android:drawable="@drawable/md_switch_thumb_off_normal" />
</selector>